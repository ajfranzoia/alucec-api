    "express": "^4.13.3",
    "express-session": "^1.11.3",
    "glob": "^5.0.3",
    "jsonwebtoken": "^5.0.5",
    "lodash": "^3.10.1",
    "method-override": "^2.3.0",
    "moment": "^2.10.6",
    "mongoose": "^4.1.2",
    "mongoose-paginate": "^4.2.0",
    "mongoose-unique-validator": "^0.4.1",
